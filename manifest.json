{
    "name": "MewMewTyping",
    "description": "タイピングの際に猫が鳴きます。",
    "version": "1.0.0",
    "manifest_version": 3,
    "web_accessible_resources": [{
        "resources": ["resource/sound/neko.mp3"],
        "matches": ["<all_urls>"]
    }],
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":[
                "js/content_script.js"
            ]
        }
    ],
    "icons":{
        "16":"resource/icons/mewmew16.png",
        "48":"resource/icons/mewmew48.png",
        "128":"resource/icons/mewmew128.png"
    }
}